<sqlMap namespace="Work">
	<typeAlias alias="SW_Work" type="kr.mentalcare.project.model.SW_Work"/>
	<select id="getRecentWork" resultClass="SW_Work">
	SELECT * FROM SW_WORK
	ORDER BY num DESC
